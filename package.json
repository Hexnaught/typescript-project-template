    "serve": "node .",
    "clean:build:serve": "yarn clean && yarn build && yarn serve",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts'",
    "prettier:fix": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "jest"
